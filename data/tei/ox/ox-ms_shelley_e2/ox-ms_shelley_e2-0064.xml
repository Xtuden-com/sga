<surface xmlns="http://www.tei-c.org/ns/1.0" xmlns:mith="http://mith.umd.edu/sc/ns1#" ulx="0"
  uly="0" lrx="5380" lry="6834" xml:id="ox-ms_shelley_e2-0064" partOf="#ox-ms_shelley_e2" mith:folio="31v" mith:shelfmark="MS. Shelley e. 2">
  <graphic url="https://iiif.bodleian.ox.ac.uk/iiif/image/d7b4f7a3-674c-4861-aae2-57560e9437af"/>
  <zone type="main">
    <line rend="indent1">Through the noontide darkness deep</line>
    <line rend="indent1">By the odour breathing sleep</line>
    <line rend="indent1">Of <mod>
        <del rend="strikethrough">sweet</del>
        <add place="superlinear">faint</add>
      </mod> night <unclear reason="illegible">-</unclear>flowers, &amp; the waves</line>
    <line rend="indent1"><mod>
        <del rend="overwritten">Of</del>
        <add place="intralinear">At</add>
      </mod> the fountain-lighted caves</line>
    <line rend="indent1">While <mod>
        <del rend="smear">the</del>
        <add place="superlinear">our</add>
      </mod> music wild &amp; sweet</line>
    <line rend="indent1">Mock<mod>
        <del rend="overwritten">ed</del>
        <add place="intralinear">s</add>
      </mod> thy gently-falling feet</line>
    <line rend="center">Child of Ocean!</line>
    <milestone unit="tei:speaker" spanTo="#e2-0064.01"/>
    <line rend="center">Asia</line>
    <anchor xml:id="e2-0064.01"/>
    <line>Shall we pursue the sound <unclear reason="illegible">?—</unclear> it grows more
      faint</line>
    <line>And distant</line>
    <milestone unit="tei:speaker" spanTo="#e2-0064.02"/>
    <line rend="center">Panthea</line>
    <anchor xml:id="e2-0064.02"/>
    <line rend="indent3">list — <mod>
        <del rend="overwritten">it</del>
        <add place="intralinear">the strain</add>
      </mod> floats nearer now</line>
    <milestone unit="tei:speaker" spanTo="#e2-0064.03"/>
    <line rend="center">Echo</line>
    <anchor xml:id="e2-0064.03"/>
    <line rend="indent2">In the world unknown</line>
    <line rend="indent2">Sleeps a voice unspoken</line>
    <line rend="indent2">By thy step alone</line>
    <line rend="indent2">Can its rest be broken</line>
    <line rend="center">Child of Ocean</line>
    <milestone unit="tei:speaker" spanTo="#e2-0064.04"/>
    <line rend="center">Asia</line>
    <anchor xml:id="e2-0064.04"/>
    <line>How the notes sink upon the ebbing wind</line>
    <milestone unit="tei:speaker" spanTo="#e2-0064.05"/>
    <line rend="center"><retrace cause="unclear">E</retrace>cho</line>
    <anchor xml:id="e2-0064.05"/>
    <line rend="indent3">O follow follow</line>
    <line rend="indent3">Thro the caverns hollow</line>
    <line rend="right"><add><metamark function="count">190</metamark></add></line>
  </zone>
</surface>
