  uly="0" lrx="5410" lry="6872" xml:id="ox-ms_shelley_e2-0081" partOf="#ox-ms_shelley_e2" mith:folio="40r" mith:shelfmark="MS. Shelley e. 2">
  <graphic url="https://iiif.bodleian.ox.ac.uk/iiif/image/d2c6d8b8-a015-4e8a-8474-3f0b8f6d91eb"/>
  <zone type="library"><line>40</line></zone>
  <zone type="main">
    <milestone unit="tei:speaker" spanTo="#e2-0081.01"/>
    <line rend="center">Song of Spirits</line>
    <anchor xml:id="e2-0081.01"/>
    <line rend="indent1">To the Deep, to the Deep</line>
    <line rend="indent2">Down, Down!</line>
    <line rend="indent1">Through the shade of <mod>
        <del rend="overwritten">s</del>
        <add place="intralinear">S</add>
      </mod>leep</line>
    <line rend="indent1">Through the cloudy strife</line>
    <line rend="indent1">Of Death and of life</line>
    <line rend="indent1">Through the viel &amp; the bar</line>
    <line rend="indent1">Of things which seem &amp; are</line>
   
    <line>Even <add place="superlinear"><del rend="strikethrough">Desideratum</del></add> to
      <mod>
        <del rend="strikethrough">Desideratur aliquid</del>
        <add place="superlinear">the <del rend="strikethrough">threshold of the</del></add>
      </mod></line>
    <line rend="indent3">steps of the remotest Throne</line>
    <line rend="indent2">Down, Down!</line>
    <space dim="vertical" unit="lines" extent="1"/>
    <line rend="indent1">While the sound, whirls around,</line>
    <line rend="indent2">Down, Down!</line>
    <line rend="indent1">As the fawn draw<add>s</add> the hound</line>
    <line rend="indent1">As the lightning the vapour</line>
    <line rend="indent1">As a weak moth the taper</line>
    <line rend="indent1">Death <mod>
        <del rend="overwritten">d</del>
        <add place="intralinear">D</add>
      </mod>espair, Love Sorrow</line>
    <line rend="indent1">Time both, To-day to-morrow;</line>
    <line>As steel obeys the <mod>
        <del rend="overwritten">s</del>
        <add place="intralinear">S</add>
      </mod>pirit of the stone</line>
    <line rend="indent3">Down, Down</line>
  </zone>
</surface>
