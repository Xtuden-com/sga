<surface xmlns="http://www.tei-c.org/ns/1.0" xmlns:mith="http://mith.umd.edu/sc/ns1#" ulx="0" uly="0" lrx="5406" lry="6876" xml:id="ox-ms_shelley_e1-0009" partOf="#ox-ms_shelley_e1"   mith:folio="4r" mith:shelfmark="MS. Shelley e. 1">
